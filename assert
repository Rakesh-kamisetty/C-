#include <assert.h>
#include <stdio.h>
int main () {
   int a;
   float b;
	 
   printf("Enter an integer value: ");
   scanf("%d", &a);
   assert(a >= 10);  /*If expression is FALSE, assert() displays an error 
                        message and aborts program execution.*/
   printf("Integer entered is %d\n", a);
    
   printf("Enter a float value: ");
   scanf("%f",&b);
   assert(b<=1.0);
   printf("String entered is: %f\n", b);
	
   return 0;
}
